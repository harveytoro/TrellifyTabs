{
  "manifest_version": 2,
  "name": "Trellify Tabs",
  "description": "Chrome extension to save and restore tabs using Trello",
  "version": "1.0",
  "background": {
     "scripts": [
       "js/trellifyBackground.js"
     ],
     "persistent": false
  },
  "options_page": "html/page_options.html",
  "browser_action": {
    "default_icon": "icons/icon.png",
    "default_popup": "html/page_popup.html"
  },
  "permissions": [
    "tabs"
  ],
  "web_accessible_resources": [
         "html/page_options.html"
    ]
}
